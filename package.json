{
  "name": "undux",
  "version": "5.0.0-alpha.2",
  "description": "Dead simple state management for React",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bcherny/undux.git"
  },
  "bugs": {
    "url": "https://github.com/bcherny/undux/issues"
  },
  "homepage": "https://github.com/bcherny/undux#readme",
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -d && flow focus-check src/index.js.flow && cp src/index.js.flow dist/src/index.js.flow",
    "clean": "shx rm -rf dist",
    "format": "prettier --write src/**/* test/**/*",
    "lint": "tslint -p tsconfig.json src/*.ts src/*.tsx src/plugins/*.ts",
    "prepublishOnly": "npm test",
    "pretest": "npm run build",
    "tdd": "concurrently -k \"tsc -w\" \"ava -w\"",
    "test": "ava && flow focus-check ./test/test.flow.js && ./test/bad-cases/run.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jsdom": "^11.0.6",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "ava": "^0.25.0",
    "concurrently": "^3.6.1",
    "flow-bin": "^0.77.0",
    "immutable": "^3.8.2",
    "jsdom": "^11.12.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rxjs": "^6.2.2",
    "shx": "^0.3.2",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "rxjs": ">=5.0.0"
  },
  "keywords": [
    "redux",
    "flux",
    "rx",
    "reactive",
    "simple",
    "model",
    "state",
    "store"
  ],
  "ava": {
    "files": [
      "./dist/test/test.js"
    ],
    "source": [
      "./dist/src/*.js"
    ],
    "verbose": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
